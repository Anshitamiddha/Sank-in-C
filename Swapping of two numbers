#include<stdio.h>
int main()
{
    int a,b,temp=1;
    printf("Enter two number:-");
    scanf("%d %d",&a,&b);
    printf("Before Swapping: value of a is %d , value of b is %d",a,b);
    temp=a;
    a=b;
    b=temp;
    printf("\nAfter swapping : value of a is %d , value of b is %d",a,b);
    return 0;

    
}
